pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh 'echo 'hello world' '
            }
        }
        stage('Test') { 
            steps {
                sh 'echo 'test stage''
            }
        }
        stage('Deploy') { 
            steps {
                sh 'echo 'deploy stage''
            }
        }
    }
}

